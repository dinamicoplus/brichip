TARGET=@PACKAGE_NAME@
VERSION=@PACKAGE_VERSION@
HOST=@HOST@
srcdir=@srcdir@
CFLAGS=@CFLAGS@
LDFLAGS=@LDFLAGS@
INCLUDES=@INCLUDES@
LIBS=@LIBS@

SOURCES=@SOURCES@
OBJECTS=@OBJECTS@
OUTPUT_FORMAT=@OUTPUT_FORMAT@
EXECUTABLE_NAME=@EXECUTABLE_NAME@

all: $(OBJECTS)
	test -d bin || mkdir bin
	gcc -o bin/$(EXECUTABLE_NAME) $(OBJECTS) $(LIBS) $(LDFLAGS)

$(OBJECTS):
	gcc $(CFLAGS) $(INCLUDES) -o $(OBJECTS) -c $(SOURCES)

clean:
	rm -r bin
@EXTRA@